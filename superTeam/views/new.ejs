<%- include("./partials/header")%>
   

    <div class="mt-5">
        <form method=POST action="/cohort">
          <div class="form-group">
            <label for="imgUrl">Image URL:</label>
            <input type="text" class="form-control" id="imgUrl" name="imgUrl" placeholder="img url">
          </div>
          <div class="form-group">
            <label for="name">Name:</label>
            <input type="text" class="form-control" id="name" name="name" placeholder="Team Name">
          </div>
          <div class="form-group">
            <label for="members">Members</label>
            <textarea class="form-control" id="exampleFormControlTextarea1" 
            id="members" name="members" placeholder="ex: Erika, Juliana, Elise, Luciana,..." 
            rows="3"></textarea>
          </div>

          <button type="submit" class="btn btn-secondary">Submit</button>
      </form>
    </div>
    

<%- include("./partials/footer")%>

